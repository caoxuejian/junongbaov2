package com.nxt.nxtapp.model.publicmsg;

/*
 * 用户回复信息
 */
public class Huifu {
    private String id;
	private String pinglunren;// 评论者
	private String huifuzhe;// 回复者
	private String content;// 内容
	private String huifu;// 回复

	public Huifu() {
		super();
	}

	public Huifu(String pinglunren, String huifuzhe, String content,
			String huifu) {
		super();
		this.pinglunren = pinglunren;
		this.huifuzhe = huifuzhe;
		this.content = content;
		this.huifu = huifu;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getHuifu() {
		return huifu;
	}

	public void setHuifu(String huifu) {
		this.huifu = huifu;
	}

	public String getPinglunren() {
		return pinglunren;
	}

	public void setPinglunren(String pinglunren) {
		this.pinglunren = pinglunren;
	}

	public String getHuifuzhe() {
		return huifuzhe;
	}

	public void setHuifuzhe(String huifuzhe) {
		this.huifuzhe = huifuzhe;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	@Override
	public String toString() {
		return "Huifu [pinglunren=" + pinglunren + ", huifuzhe=" + huifuzhe
				+ ", content=" + content + ", huifu=" + huifu + "]";
	}

}
