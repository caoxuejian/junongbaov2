package com.nxt.nxtapp.model;

import java.io.Serializable;

public class NewsRoot implements Serializable{
private String news;
private String recnums;
private String list_data;
private String todaynews;

public String getTodaynews() {
	return todaynews;
}
public void setTodaynews(String todaynews) {
	this.todaynews = todaynews;
}
public String getList_data() {
	return list_data;
}
public void setList_data(String list_data) {
	this.list_data = list_data;
}
public String getNews() {
	return news;
}
public void setNews(String news) {
	this.news = news;
}
public String getRecnums() {
	return recnums;
}
public void setRecnums(String recnums) {
	this.recnums = recnums;
}
@Override
public String toString() {
	return "NewsRoot [news=" + news + ", recnums=" + recnums + ", list_data="
			+ list_data + ", todaynews=" + todaynews + "]";
}

}
