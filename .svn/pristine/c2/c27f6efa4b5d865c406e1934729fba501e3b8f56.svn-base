package com.nxt.nxtapp.model.publicmsg;

import java.util.ArrayList;
import java.util.List;


import android.graphics.Bitmap;
import android.graphics.drawable.Drawable;
public class GQDatas {
	private String name;// 维修站点的企业名称
	private String level;// 维修站点的类别及等级
	private String scope;// 维修站点的维修范围
	private String pic;// 维修站点的经营场地照片
	private String workendtime;// 预期作业结束时间
	private String workstarttime;// 语气作业开始时间
	private String pubtime; // 信息发布时间
	private String address; // 信息发布者地区，引进，是作业地区，派出，是派出地区
	private String lat; // 上面的地址纬度
	private String lng; // 上面的地址的经度
	private String tel; // 联系人电话
	private String contacts; // 联系人
	private String count;// 派出或引进的农机数量
	private String type; // 农机种类
	private String title; // 显示的标题
	private String direction; // 派出还是引进，派出 out，引进 in
	private String id; // 信息的id
	private String distance = "320"; // 距离
	private String image_src; // 图片地址
	private String url;// 信息链接地址
	private String sort;// 供应还是求购
	private int recnums;// 总记录数量
	private String buyorsell;// sell or buy
	private String content;// 发布信息的内容
	private Bitmap bitmap;// 缓存图片
	private List<Huifu> huifus = new ArrayList<Huifu>();
	private Drawable drawable;
	private List<Picture> mlist;

	
	public List<Picture> getMlist() {
		return mlist;
	}

	public void setMlist(List<Picture> mlist) {
		this.mlist = mlist;
	}

	public Drawable getDrawable() {
		return drawable;
	}

	public void setDrawable(Drawable drawable) {
		this.drawable = drawable;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLevel() {
		return level;
	}

	public void setLevel(String level) {
		this.level = level;
	}

	public String getScope() {
		return scope;
	}

	public void setScope(String scope) {
		this.scope = scope;
	}

	public String getPic() {
		return pic;
	}

	public void setPic(String pic) {
		this.pic = pic;
	}

	public List<Huifu> getHuifus() {
		return huifus;
	}

	public void setHuifus(List<Huifu> huifus) {
		this.huifus = huifus;
	}

	public Bitmap getBitmap() {
		return bitmap;
	}

	public void setBitmap(Bitmap bitmap) {
		this.bitmap = bitmap;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getBuyorsell() {
		return buyorsell;
	}

	public void setBuyorsell(String buyorsell) {
		this.buyorsell = buyorsell;
	}

	public String getSort() {
		return sort;
	}

	public void setSort(String sort) {
		this.sort = sort;
	}

	public String getWorkendtime() {
		return workendtime;
	}

	public void setWorkendtime(String workendtime) {
		this.workendtime = workendtime;
	}

	public String getWorkstarttime() {
		return workstarttime;
	}

	public void setWorkstarttime(String workstarttime) {
		this.workstarttime = workstarttime;
	}

	public String getPubtime() {
		return pubtime;
	}

	public void setPubtime(String pubtime) {
		this.pubtime = pubtime;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public String getLat() {
		return lat;
	}

	public void setLat(String lat) {
		this.lat = lat;
	}

	public String getLng() {
		return lng;
	}

	public void setLng(String lng) {
		this.lng = lng;
	}

	public String getTel() {
		return tel;
	}

	public void setTel(String tel) {
		this.tel = tel;
	}

	public String getContacts() {
		return contacts;
	}

	public void setContacts(String contacts) {
		this.contacts = contacts;
	}

	public String getCount() {
		return count;
	}

	public void setCount(String count) {
		this.count = count;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getDirection() {
		return direction;
	}

	public void setDirection(String direction) {
		this.direction = direction;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public int getRecnums() {
		return recnums;
	}

	public void setRecnums(int recnums) {
		this.recnums = recnums;
	}

	public String getDistance() {
		return distance;
	}

	public void setDistance(String distance) {
		this.distance = distance;
	}

	public String getImage_src() {
		return image_src;
	}

	public void setImage_src(String image_src) {
		this.image_src = image_src;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	@Override
	public String toString() {
		return "Datas [name=" + name + ", level=" + level + ", scope=" + scope
				+ ", pic=" + pic + ", workendtime=" + workendtime
				+ ", workstarttime=" + workstarttime + ", pubtime=" + pubtime
				+ ", address=" + address + ", lat=" + lat + ", lng=" + lng
				+ ", tel=" + tel + ", contacts=" + contacts + ", count="
				+ count + ", type=" + type + ", title=" + title
				+ ", direction=" + direction + ", id=" + id + ", distance="
				+ distance + ", image_src=" + image_src + ", url=" + url
				+ ", sort=" + sort + ", recnums=" + recnums + ", buyorsell="
				+ buyorsell + ", content=" + content + ", bitmap=" + bitmap
				+ ", huifus=" + huifus + ", drawable=" + drawable + ", mlist="
				+ mlist + "]";
	}



}
